# Variables d'environnement pour Vercel
# Copiez ce fichier vers .env.local pour le d√©veloppement local
# Configurez ces variables dans l'interface Vercel pour la production

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Environment
NODE_ENV=production

# Next.js Configuration
NEXT_PUBLIC_APP_URL=https://your-app.vercel.app

# Optional: Analytics
NEXT_PUBLIC_GA_ID=your_google_analytics_id

# Optional: Sentry (Error tracking)
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn

# Optional: Feature flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_ERROR_TRACKING=false

# Database (if using external database)
DATABASE_URL=your_database_url

# Authentication (if using external auth)
AUTH_SECRET=your_auth_secret
AUTH_URL=https://your-app.vercel.app

# Email (if using email service)
SMTP_HOST=your_smtp_host
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASS=your_smtp_password

# File Storage (if using external storage)
STORAGE_BUCKET=your_storage_bucket
STORAGE_REGION=your_storage_region
STORAGE_ACCESS_KEY=your_storage_access_key
STORAGE_SECRET_KEY=your_storage_secret_key
